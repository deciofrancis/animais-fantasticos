{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-console": 0
    },
    "type": "array",
    "items": [
        {
        "type": "number",
        "minimum": 1
        },
        {
        "type": "string",
        "enum": [
            "exclude-class-expressions"
        ]
        }
    ],
    "additionalItems": false,
    "minLength": 1,
    "maxLength": 2
}
